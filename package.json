{
  "name": "react-start-kit",
  "version": "1.0.0",
  "description": "A starter kit for React and Redux applications.",
  "main": "./src/index.js",
  "scripts": {
    "dev": "better-npm-run dev",
    "start": "better-npm-run start",
    "build": "better-npm-run build",
    "test": "better-npm-run test",
    "test:coverage": "better-npm-run test:coverage",
    "format": "prettier --write 'src/**/*.js'",
    "lint:js": "eslint 'src/**/*.js' --fix",
    "lint:styles": "stylelint **/*.scss"
  },
  "betterScripts": {
    "dev": {
      "command": "webpack-dev-server",
      "env": {
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "start": {
      "command": "node server.js",
      "env": {
        "HOST": "http://localhost:",
        "NODE_ENV": "production",
        "PORT": 8080
      }
    },
    "build": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "jest --config ./jest.json --watchAll",
      "env": {
        "NODE_ENV": "testing"
      }
    },
    "test:coverage": {
      "command": "jest --config ./jest.json --coverage",
      "env": {
        "NODE_ENV": "testing"
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write --single-quote",
      "eslint --fix",
      "jest --config ./jest.json /src",
      "git add"
    ],
    "*.scss": [
      "stylelint **/*.scss",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": "https://github.com/mattcarlotta/Webpack-React-Boilerplate",
  "author": "Matt Carlotta",
  "license": "ISC",
  "private": false,
  "engines": {
    "node": ">=8",
    "npm": ">=3"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "better-npm-run": "^0.1.1",
    "connected-react-router": "^6.3.1",
    "express": "^4.16.4",
    "history": "^4.7.2",
    "lodash": "4.17.11",
    "morgan": "^1.9.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "^6.0.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-form": "^8.1.0",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.1.3"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "devDependencies": {
    "@babel/core": "7.3.4",
    "@babel/plugin-proposal-class-properties": "7.3.4",
    "@babel/plugin-proposal-decorators": "7.3.0",
    "@babel/plugin-proposal-export-namespace-from": "7.2.0",
    "@babel/plugin-proposal-function-sent": "7.2.0",
    "@babel/plugin-proposal-json-strings": "7.2.0",
    "@babel/plugin-proposal-numeric-separator": "7.2.0",
    "@babel/plugin-proposal-throw-expressions": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-syntax-import-meta": "7.2.0",
    "@babel/plugin-transform-runtime": "7.3.4",
    "@babel/polyfill": "7.2.5",
    "@babel/preset-env": "7.3.4",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.0.0",
    "@babel/runtime": "7.3.4",
    "babel-core": "^7.0.0-beta.4",
    "babel-eslint": "10.0.1",
    "babel-jest": "^24.3.1",
    "babel-loader": "8.0.5",
    "babel-plugin-lodash": "3.3.4",
    "babel-preset-env": "^1.7.0",
    "browserslist": "4.4.2",
    "clean-webpack-plugin": "^2.0.0",
    "css-loader": "^2.1.1",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.10.0",
    "error-overlay-webpack-plugin": "^0.1.6",
    "eslint": "^5.15.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-loader": "2.1.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jest": "22.3.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-watch": "^5.0.1",
    "file-loader": "3.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.3.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "^24.3.1",
    "jest-css-modules-transform": "^2.1.1",
    "jest-enzyme": "^7.0.2",
    "jsdom": "^13.2.0",
    "lint-staged": "^8.1.5",
    "mini-css-extract-plugin": "0.5.0",
    "node-sass": "4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "1.16.4",
    "prop-types": "^15.7.2",
    "react-dev-utils": "^8.0.0",
    "redux-devtools-extension": "^2.13.8",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-scss": "^3.5.4",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpackbar": "^3.1.5"
  }
}
